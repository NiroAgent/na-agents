{
  "name": "na-agents",
  "version": "2.0.1",
  "description": "Multi-Agent AI System for Dashboard Integration",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start:heartbeat": "ts-node agent-heartbeat.ts",
    "dev:heartbeat": "nodemon --exec ts-node agent-heartbeat.ts",
    "start:all": "npm run start:architect & npm run start:developer & npm run start:devops & npm run start:qa & npm run start:manager",
    "start:architect": "node dist/agents/architect-agent.js",
    "start:developer": "node dist/agents/developer-agent.js",
    "start:devops": "node dist/agents/devops-agent.js",
    "start:qa": "node dist/agents/qa-agent.js",
    "start:manager": "node dist/agents/manager-agent.js",
    "dev:architect": "nodemon --exec ts-node src/agents/architect-agent.ts",
    "dev:developer": "nodemon --exec ts-node src/agents/developer-agent.ts",
    "dev:devops": "nodemon --exec ts-node src/agents/devops-agent.ts",
    "dev:qa": "nodemon --exec ts-node src/agents/qa-agent.ts",
    "dev:manager": "nodemon --exec ts-node src/agents/manager-agent.ts",
    "test": "jest",
    "test:regression": "node regression-test.js",
    "test:regression:dev": "node regression-test.js --url https://dev-agents.niroagent.com --threshold 70",
    "test:regression:stg": "node regression-test.js --url https://stg-agents.niroagent.com --threshold 80",
    "test:regression:prd": "node regression-test.js --url https://agents.niroagent.com --threshold 95",
    "test:heartbeat": "node test-heartbeat.js",
    "test:copilot": "node test-copilot.js",
    "test:assignment": "./test-agent-assignment.sh",
    "test:all": "npm run test && npm run test:regression",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "health-check": "node -e \"console.log('Health check passed')\""
  },
  "dependencies": {
    "@aws-sdk/client-batch": "^3.450.0",
    "@aws-sdk/client-cloudwatch": "^3.450.0",
    "@aws-sdk/client-dynamodb": "^3.872.0",
    "@aws-sdk/client-ec2": "^3.450.0",
    "@aws-sdk/client-ecs": "^3.450.0",
    "@aws-sdk/client-lambda": "^3.450.0",
    "@aws-sdk/lib-dynamodb": "^3.872.0",
    "@octokit/rest": "^20.0.2",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "node-schedule": "^2.1.1",
    "p-queue": "^8.1.0",
    "socket.io-client": "^4.5.4",
    "uuid": "^9.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/node-schedule": "^2.1.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
